{
  "name": "connect-initial-props",
  "version": "0.1.0",
  "description": "A decorator for Next.js and React-Redux apps to connect `getInitialProps` to `state` and `dispatch`",
  "repository": "git@github.com:webiya/connect-initial-props.git",
  "author": "Igal Steklov <igal@webiya.co.il>",
  "license": "MIT",
  "keywords": [
    "react",
    "redux",
    "react-redux",
    "next.js"
  ],
  "main": "./lib/index.js",
  "module": "es/index.js",
  "files": [
    "lib",
    "src",
    "es"
  ],
  "scripts": {
    "build:commonjs": "cross-env BABEL_ENV=commonjs babel src --out-dir lib",
    "build:es": "babel src --out-dir es",
    "build": "npm run build:commonjs && npm run build:es",
    "clean": "rimraf lib es",
    "format": "prettier --write \"src/**/*.js\"",
    "lint": "eslint src",
    "prepare": "npm run clean && npm run build"
  },
  "dependencies": {
    "@babel/runtime": "^7.9.2",
    "react-redux": "^7.2.0",
    "redux": "^4.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-proposal-decorators": "^7.8.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.9.5",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.5",
    "babel-eslint": "^10.1.0",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-import": "^2.20.2",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.4",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0"
  }
}
